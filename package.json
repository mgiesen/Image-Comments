{
	"name": "image-comments",
	"displayName": "Image Comments",
	"icon": "icon.png",
	"version": "0.7.1",
	"description": "This Visual Studio Code extension lets you easily add visual comments such as sketches or diagrams directly into your source code, improving code documentation and understanding.",
	"publisher": "mgiesen",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/mgiesen/Image-Comments.git"
	},
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Programming Languages",
		"Formatters",
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:python",
		"onLanguage:cpp",
		"onLanguage:csharp",
		"onLanguage:sql",
		"onLanguage:typescript",
		"onLanguage:typescriptreact",
		"onLanguage:php",
		"onLanguage:java",
		"onLanguage:ruby",
		"onLanguage:go",
		"onLanguage:swift",
		"onLanguage:kotlin",
		"onLanguage:perl",
		"onLanguage:r",
		"onLanguage:shellscript",
		"onLanguage:lua",
		"onLanguage:groovy",
		"onLanguage:powershell",
		"onLanguage:rust",
		"onLanguage:dart",
		"onLanguage:haskell",
		"onLanguage:elixir"
	],
	"main": "./extension.js",
	"contributes": {
		"hoverProvider": [
			{
				"for": [
					"javascript",
					"python",
					"cpp",
					"csharp",
					"sql",
					"typescript",
					"typescriptreact",
					"php",
					"java",
					"ruby",
					"go",
					"swift",
					"kotlin",
					"perl",
					"r",
					"shellscript",
					"lua",
					"groovy",
					"powershell",
					"rust",
					"dart",
					"haskell",
					"elixir"
				],
				"type": "markdown"
			}
		]
	},
	"devDependencies": {
		"vscode": "^1.1.34"
	},
	"dependencies": {
		"jimp": "^0.3.5",
		"sharp": "^0.32.6"
	}
}
